<main class="py-5">
    <section>
        <div class="card">
            <div class="nav">
                {{#each navigation.links}}
                <div class="nav-item">
                    {{#if href}}
                    <a href="{{href}}" class="nav-link">{{text}}</a>
                    {{else}}
                    <a href="#" class="nav-link disabled">{{text}}</a>
                    {{/if}}
                </div>
                {{/each}}
            </div>
        </div>
    </section>

    <section>
        <h1>{{header}}</h1>
    </section>

    <section class="row mb-5">
        <section class="col-8">

            <div class="mb-3">
                {{#with contacts_data}}
                <div class="mb-3">
                    <h2>{{title}}</h2>
                </div>
                <div class="mb-3">
                    <div class="row">
                        {{#each buttons}}
                        <div class="col-6">
                            {{#if isPrimary}}<button class="btn btn-warning">{{text}}</button>{{/if}}
                            {{#if isOutline}}<button class="btn btn-outline-warning">{{text}}</button>{{/if}}
                        </div>
                        {{/each}}
                    </div>
                </div>
                {{/with}}
            </div>

            <div>
                {{#with contacts_data.form.input}}
                <div class="row">
                    <div class="col-6">
                        {{#with surname}}
                        <form>
                            <label class="form-label">{{label}}</label>
                            </text><input class="form-control">
                            <div class="form-text text-danger">{{caption}}</div>
                        </form>
                        {{/with}}
                    </div>

                    <div class="col-6">
                        {{#with name}}
                        <form>
                            <label class="form-label">{{label}}</label>
                            </text><input class="form-control">
                            <div class="form-text text-danger">{{caption}}</div>
                        </form>
                        {{/with}}
                    </div>
                </div>
                <div class="row">
                    <div class="col-6">
                        {{#with phone}}
                        <form>
                            <label class="form-label">{{label}}</label>
                            </text><input class="form-control">
                            <div class="form-text text-danger">{{caption}}</div>
                        </form>
                        {{/with}}
                    </div>

                    <div class="col-6">
                        {{#with email}}
                        <form>
                            <label class="form-label">{{label}}</label>
                            </text><input class="form-control">
                            <div class="form-text text-danger">{{caption}}</div>
                        </form>
                        {{/with}}
                    </div>
                </div>
                {{/with}}
            </div>

            <div>
                <hr>
                {{#each contacts_data.form.switch}}
                <div class="form-check form-switch">
                    <label class="form-check-label">{{text}}</label>
                    {{#if isActive}}<input name="{{isActive}}" type="checkbox" class="form-check-input">{{/if}}
                </div>
                {{/each}}
            </div>

            <div>
                {{#with delivery}}
                <div>
                    <h2>{{title}}</h2>
                </div>
                <div class="card g-5">
                    <div class="card-body row">
                        {{#each select}}
                        <div class="col-6 pt-5">
                            <div class="form-label">{{label}}</div>
                            <select class="form-select">
                                <option selected>{{placeholder}}</option>
                                <div>{{#each options}}
                                    <option value="{{value}}">{{text}}</option>
                                    {{/each}}
                                </div>
                            </select>
                        </div>
                        {{/each}}
                    </div>
                </div>

                <div>
                    {{#each delivery_type}}
                    <div class="form-check">
                        <label class="form-check-label">{{type}}</label>
                        <input name="{{type}}" type="radio" class="form-check-input">
                    </div>
                    {{/each}}
                </div>
                <hr>

                <div>
                    {{#with payment}}
                    <div>
                        <h2>{{title}}</h2>
                    </div>
                    {{#each list}}
                    <div class="form-check">
                        <label class="form-check-label">{{type}}</label>
                        <input name="{{type}}" type="radio" class="form-check-input">
                        <div>{{description}}</div>
                    </div>
                    {{/each}}
                    {{/with}}
                </div>
                <hr>

                <div>
                    {{#with contact}}
                    <div>
                        <h2>{{title}}</h2>
                    </div>
                    <div class="row">
                        <div class="col-6">
                            {{#with surname}}
                            <form>
                                <label class="form-label">{{label}}</label>
                                </text><input class="form-control">
                                <div class="form-text">{{caption}}</div>
                            </form>
                            {{/with}}
                        </div>

                        <div class="col-6">
                            {{#with name}}
                            <form>
                                <label class="form-label">{{label}}</label>
                                </text><input class="form-control">
                                <div class="form-text">{{caption}}</div>
                            </form>
                            {{/with}}
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-6">
                            {{#with phone}}
                            <form>
                                <label class="form-label">{{label}}</label>
                                </text><input class="form-control">
                                <div class="form-text">{{caption}}</div>
                            </form>
                            {{/with}}
                        </div>

                        <div class="col-6">
                            {{#with email}}
                            <form>
                                <label class="form-label">{{label}}</label>
                                <input text="{{placeholder}}" class="form-control">
                                <div class="form-text">{{caption}}</div>
                            </form>
                            {{/with}}
                        </div>
                    </div>
                    {{/with}}
                </div>
                <hr>

                <div>
                    {{#with coment}}
                    <form>
                        <label class="form-label">{{label}}</label>
                        <input text="{{placeholder}}" class="form-control">
                    </form>
                    {{/with}}
                </div>
                <hr>
                {{/with}}
            </div>

        </section>

        <section class="col-4">
            <div class="card mb-3">
                {{#each action_block}}
                <div class="card-body">
                    {{#if isSuccess}}<button class="btn btn-success">{{text}}</button>{{/if}}
                    {{#if isDanger}}<button class="btn btn-danger">{{text}}</button>{{/if}}
                    {{#if isInfo}}<button class="btn btn-info">{{text}}</button>{{/if}}
                </div>
                {{/each}}
            </div>

            <div class="card">
                {{#with total}}
                <div class="card-body">
                    <div class="card-title">
                        <h2>{{title}}</h2>
                    </div>
                    {{#each item_list}}
                    <p>
                        <il>{{name}}: {{price}}</il>
                    </p>
                    {{/each}}
                    <hr>
                    {{#each additional_info}}
                    <p>
                        <il>{{text}}: {{value}}</il>
                    </p>
                    {{/each}}
                    <hr>
                    {{#with total_price}}
                    <p>
                        <il>{{text}}: {{value}}</il>
                    </p>
                    {{/with}}
                    <hr>
                    {{#each button}}
                    <div class="p-2">
                        {{#if isPrimary}}<button class="btn btn-primary">{{text}}</button>{{/if}}
                        {{#if isOutline}}<button class="btn btn-outline-primary">{{text}}</button>{{/if}}
                    </div>
                    {{/each}}
                </div>
                {{/with}}
            </div>
        </section>

    </section>




    <section>
        <div class="col-8">
            {{#with coupon}}
            <div>
                <h2>{{title}}</h2>
            </div>
            {{#with select}}
            <div>
                {{#with availabel_coupon}}
                <div>
                    <label class="form-label">{{label}}</label>
                    <select class="form-select">
                        <option selected>{{placeholder}}</option>
                        {{#each options}}
                        <option value="{{value}}">{{text}}</option>
                        {{/each}}
                    </select>
                </div>
                {{/with}}
            </div>
            <div>
                {{#with friend_coupon}}
                <div>
                    <label class="form-label">{{label}}</label>
                    <select class="form-select">
                        <option selected>{{placeholder}}</option>
                        {{#each options}}
                        <option value="{{value}}">{{text}}</option>
                        {{/each}}
                    </select>
                </div>
                {{/with}}
            </div>
            {{/with}}
            <div>
                {{#with add}}
                <div>
                    {{#with friend}}
                    <form>
                        <label class="form-label">{{label}}</label>
                        <input text="{{placeholder}}" class="form-control">
                        <div class="form-text">{{caption}}</div>
                    </form>
                    {{/with}}
                </div>
                <div>
                    {{#with colleague}}
                    <form>
                        <label class="form-label">{{label}}</label>
                        <input text="{{placeholder}}" class="form-control">
                        <div class="form-text">{{caption}}</div>
                    </form>
                    {{/with}}
                </div>
                {{/with}}
            </div>
            {{/with}}
            <hr>
        </div>
    </section>

    <section>
        <div class="col-8">
            {{#with quiz}}
            <div>
                <h2>{{title}}</h2>
            </div>
            <div>
                {{#with preference}}
                <div>
                    <div class="form-label">{{title}}</div>
                    {{#each options}}
                    <div class="form-check">
                        <label class="form-check-label">{{this}}</label>
                        <input name="food" type="radio" class="form-check-input">
                    </div>
                    {{/each}}
                </div>
                {{/with}}
            </div>
            <div>
                <form>
                    <label class="form-label">{{your_expirience.label}}</label>
                    <input text="{{placeholder}}" class="form-control">
                </form>
            </div>
            <div>
                {{#with purchase_frequency}}
                <div>
                    <label class="form-label">{{label}}</label>
                    <select class="form-select">
                        <option selected>{{placeholder}}</option>
                        {{#each options}}
                        <option value="{{value}}">{{text}}</option>
                        {{/each}}
                    </select>
                </div>
                {{/with}}
            </div>
            <div>
                {{#with factors}}
                <div>
                    <div class="form-label">{{title}}</div>
                    {{#each options}}
                    <div class="form-check">
                        <label class="form-check-label">{{this}}</label>
                        <input name="food" type="checkbox" class="form-check-input">
                    </div>
                    {{/each}}
                </div>
                {{/with}}
            </div>
            {{/with}}
        </div>
    </section>

    <section>
        <hr>
        <div class="row">
            <div class="nav">

                {{#each footer}}
                {{#with this}}
                <div class="nav-item flex-column col-2">
                    <a href="{{[0].link}}" class="nav-link">{{[0].text}}</a>
                    <a href="{{[1].link}}" class="nav-link">{{[1].text}}</a>
                    <a href="{{[2].link}}" class="nav-link">{{[2].text}}</a>
                    <a href="{{[3].link}}" class="nav-link disabled">{{[3].text}}</a>
                </div>
                {{/with}}
                {{/each}}

            </div>
        </div>
    </section>
</main>