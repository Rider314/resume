<main>
    {{#with page_header}}
    <h1 class="text-center">{{title}}</h1>
    <p class="text-center">{{description}}</p>
    {{/with}}
    {{#with section}}
    <div class="card">
        <div class="card-body">
            <div>
                {{#with about_us}}
                <div>
                    {{#with title_list}}
                    <div class="display-6 text-end">{{main}}</div>
                    <h2 class="text-end">{{sub}}</h2>
                    {{/with}}
                </div>
                <div><button class="btn btn-outline-info">{{button.text}}</button></div>
                {{/with}}
            </div>
        </div>
    </div>

    <br>

    <div class="card">
        {{#with advantage}}
        <div class="card-body">
            <div class="display-5 text-muted">{{title}}</div>
            <br>
            <div>
                {{#with efficiency}}
                <div class="card">
                    <div class="card-body">
                        <div class="display-4 text-muted">{{title}}</div>
                        <h6>{{sub_title}}</h6>
                        <p>{{description}}</p>
                        <div><button class="btn btn-outline-warning">{{button}}</button></div>
                    </div>
                </div>
                {{/with}}
            </div>
        </div>

        <div class="card-body">
            <div>
                {{#with innovation}}
                <div class="card">
                    <div class="card-body">
                        <div class="display-4 text-center text-muted">{{title}}</div>
                        <h6 class="text-center">{{sub_title}}</h6>
                        <p class="text-center">{{description}}</p>
                        <div><button class="btn btn-outline-warning">{{button}}</button></div>
                    </div>
                </div>
                {{/with}}
            </div>
        </div>

        <div class="card-body">
            <div>
                {{#with collaboration}}
                <div class="card">
                    <div class="card-body">
                        <div class="display-4 text-end text-muted">{{title}}</div>
                        <h6 class="text-end">{{sub_title}}</h6>
                        <p class="text-end">{{description}}</p>
                        <div><button class="btn btn-outline-warning">{{button}}</button></div>
                    </div>
                </div>
                {{/with}}
            </div>
        </div>
        {{/with}}
    </div>

    <br>

    <div class="card">
        {{#with agreement}}
        <div class="card-body">
            <div class="display-5">{{title}}</div>
            <br>
            <div>
                {{#with info}}
                <div class="card">
                    <div class="card-body">
                        <h5>{{sub_title}}</h5>
                        <br>
                        <div class="card">
                            <div class="card-body">
                                {{#with detail}}
                                <p>{{text}}</p>
                                <div><button class="btn btn-success">{{button}}</button></div>
                                {{/with}}
                            </div>
                        </div>
                    </div>
                </div>
                {{/with}}
            </div>
        </div>
        {{/with}}
    </div>
    {{/with}}
</main>